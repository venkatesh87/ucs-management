#!/bin/bash
## desc: Create and delete filtered object
main () {
	udm settings/packages create --position "cn=restricted,$BASE" --set name=test
	wait_listener
	neg fgrep "cn=restricted,$BASE " "$LOG"
	udm settings/packages remove --dn "cn=test,cn=restricted,$BASE"
	wait_listener
	neg fgrep "cn=restricted,$BASE " "$LOG"
}
. "${0%/*}/common.sh"
