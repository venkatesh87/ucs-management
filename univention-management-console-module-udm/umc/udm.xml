<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
	<module id="udm" version="1.0">
		<name>Univention Directory Manager</name>
		<description>Manages all UDM modules</description>
		<keywords></keywords>
		<flavor priority="50" id="users/user">
			<name>Users</name>
			<description>Management of domain users</description>
			<keywords>users</keywords>
			<categories>
				<category name="users"/>
			</categories>
		</flavor>
		<flavor priority="50" id="groups/group">
			<name>Groups</name>
			<description>Management of user and computer groups in the domain</description>
			<keywords>groups</keywords>
			<categories>
				<category name="users"/>
			</categories>
		</flavor>
		<flavor priority="50" id="computers/computer">
			<name>Computers</name>
			<description>Management of computers in the domain</description>
			<keywords>computers</keywords>
			<categories>
				<category name="devices"/>
			</categories>
		</flavor>
		<flavor priority="50" id="networks/network">
			<name>Networks</name>
			<description>Configuration of network settings in the domain</description>
			<keywords>networks</keywords>
		</flavor>
		<flavor priority="50" id="dns/dns">
			<name>DNS</name>
			<description>Configuration of DNS settings in the domain</description>
			<keywords>dns</keywords>
		</flavor>
		<flavor priority="50" id="dhcp/dhcp">
			<name>DHCP</name>
			<description>Configuration of DHCP settings in the domain</description>
			<keywords>dhcp</keywords>
		</flavor>
		<flavor priority="50" id="shares/share">
			<name>Shares</name>
			<description>Management of directory shares in the domain</description>
			<keywords>shares</keywords>
		</flavor>
		<flavor priority="50" id="shares/print">
			<name>Printers</name>
			<description>Management of printers and printer groups in the domain</description>
			<keywords>printers</keywords>
			<categories>
				<category name="devices"/>
			</categories>
		</flavor>
		<flavor priority="50" id="mail/mail">
			<name>Mail</name>
			<description>Management of mail domains and mailing lists</description>
			<keywords>mail,e-mail,email</keywords>
		</flavor>
		<flavor priority="50" id="nagios/nagios">
			<name>Nagios</name>
			<description>Configuration of Nagios settings in the domain</description>
			<keywords>nagios</keywords>
			<categories>
				<category name="devices"/>
			</categories>
		</flavor>
		<flavor priority="50" id="policies/policy">
			<name>Policies</name>
			<description>Management of domain wide policies</description>
			<keywords>policy,policies</keywords>
		</flavor>
		<flavor priority="50" id="navigation">
			<name>LDAP directory</name>
			<description>Browsing and managing the LDAP tree</description>
			<keywords>ldap,directory,udm</keywords>
		</flavor>
		<flavor priority="50" id="users/self">
			<name>User settings</name>
			<description>Change settings for the user currently logged in</description>
			<keywords>user,password</keywords>
			<categories>
				<category name="users"/>
			</categories>
		</flavor> 
		<flavor priority="50" id="license-import">
			<name>UCS license import</name>
			<description>Import new license</description>
			<keywords></keywords>
			<categories/>
			<requiredCommands>
				<requiredCommand name="udm/license/import"/>
			</requiredCommands>
		</flavor>
		<categories>
			<category name="domain"/>
		</categories>
		<command name="udm/license" function="license" />
		<command name="udm/license/info" function="license_info" />
		<command name="udm/license/import" function="license_import" />
		<command name="udm/query" function="query" />
		<command name="udm/containers" function="containers" />
		<command name="udm/types" function="types" />
		<command name="udm/network" function="network" />
		<command name="udm/properties" function="properties" />
		<command name="udm/options" function="options" />
		<command name="udm/layout" function="layout" />
		<command name="udm/values" function="values" />
		<command name="udm/templates" function="templates" />
		<command name="udm/policies" function="policies" />
		<command name="udm/put" function="put" />
		<command name="udm/add" function="add" />
		<command name="udm/remove" function="remove" />
		<command name="udm/get" function="get" />
		<command name="udm/move" function="move" />
		<command name="udm/progress" function="progress" />
		<command name="udm/validate" function="validate" />
		<command name="udm/syntax/choices/key" function="syntax_choices_key" />
		<command name="udm/syntax/choices/info" function="syntax_choices_info" />
		<command name="udm/syntax/choices" function="syntax_choices" />
		<command name="udm/nav/container/query" function="nav_container_query" />
		<command name="udm/move/container/query" function="move_container_query" />
		<command name="udm/nav/object/query" function="nav_object_query" />
		<command name="udm/object/policies" function="object_policies" />
		<command name="udm/object/options" function="object_options" />
		<command name="udm/reports/query" function="reports_query" />
		<command name="udm/reports/create" function="reports_create" />
		<command name="udm/request_new_license" function="request_new_license" />
		<command name="udm/meta_info" function="meta_info" />
	</module>
</umc>
